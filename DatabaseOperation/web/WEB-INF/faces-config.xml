<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
        http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	
	<navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>1</from-outcome>
            <to-view-id>/loginLoading.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>0</from-outcome>
            <to-view-id>/loginErr1.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>2</from-outcome>
            <to-view-id>/loginErr2.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
	<navigation-rule>
        <from-view-id>/adminLogin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>1</from-outcome>
            <to-view-id>/adminLoginLoading.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>0</from-outcome>
            <to-view-id>/loginErr2.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
	<navigation-rule>
<from-view-id>/assignmentsDisplay.xhtml</from-view-id>
<navigation-case>
<from-outcome>true</from-outcome>
<to-view-id>/adminLoginLoading.xhtml</to-view-id>
</navigation-case>
</navigation-rule>
	
    
		<navigation-rule>
        <from-view-id>/register.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>true</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

   		<navigation-rule>
        <from-view-id>/assignmentDisplayStudent.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>true</from-outcome>
            <to-view-id>/print.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Logout</from-outcome>
            <to-view-id>/indext.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    
   			<navigation-rule>
        <from-view-id>/indext.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>guest</from-outcome>
            <to-view-id>/loginLoading.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
</faces-config>

